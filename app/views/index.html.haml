%form{role:"form", action:"#{url_for('/room/new')}", method:'POST'}
  .form-group
    %label{for:"name"}
      Room Name
    %input.form-control{type:"text", id:"name", name:"name"}
  %button.btn.btn-default{type:"submit"}
    Create Room
.rooms
  -rooms.each do |room|
    .room
      %a{href:"#{url_for("/room/#{room.id}")}"}
        =room.name
